{% block slider %}
{% endblock %}
{% block change %}
{{ form_start(form,{'attr': {'novalidate': 'novalidate'}} )}}
<div class="col-lg-7 col-md-7 col-12">
    <!-- Contact Form -->
    <div class="contact-form-area m-top-30">
        <div class="row">
            <div class="col-lg-6 col-md-6 col-12">
                <div class="form-group">

                    <div class="icon"><i class="fa fa-id-card"></i></div>
                    {{ form_label(form.titre) }}
                    {{ form_widget(form.titre, {'attr': {'class': 'form-control form-control-user' , 'placeholder' : 'titre'}} ) }}

                </div>
            </div>
                <div class="col-lg-6 col-md-6 col-12">
                    <div class="form-group">
                        <div class="icon"><i class="fa fa-calendar"></i></div>
                        {{ form_label(form.date) }}
                        {{ form_widget(form.date )}}

                    </div></div>
            <div class="col-lg-6 col-md-6 col-12">
                <div class="form-group">
                    <div class="icon"> <i class="fa fa-file"></i></div>
                    {{ form_label(form.description) }}
                    {{ form_widget(form.description , {'attr': {'class': 'form-control form-control-user' , 'placeholder' : 'description'}} ) }}

                </div></div>
            <div class="col-lg-6 col-md-6 col-12">
                <div class="form-group">
                    <div class="icon"> <i class="fa fa-user"></i></div>
                    {{ form_rest(form)}}
                </div></div>
            <div class="col-12">
                <div class="form-group button">
                    <button class="bizwheel-btn theme-2">
                        {{ button_label|default('save') }}</button></div></div></div>
{{ form_end(form) }}
    </div></div>
</div>
{% endblock %}

