<form method="post" action="{{ path('delete_reclamation', {'idreclamation': reclamation.idreclamation}) }}" onsubmit="return confirm('Etes-vous sûr de vouloir supprimer cette réclamation ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ reclamation.idreclamation) }}">
    <button class="btn">Delete</button>
</form>
